<p class="gg-label mb-3">d e m o</p>
<% case microservice.microservice_category.media_type%>
<% when "image"%>
 <%= image_tag microservice.picture, alt: 'picture', class: "w-100 border rounded"%>
<% when "video"%>
  <div style="height: 30em">
    <%=microservice.youtube.html_safe %>
  </div>
<% when "sound"%>
 <%= microservice.soundcloude.html_safe  %>
<% when "text"%>
 <div class='card bg-black'>
   <div class='card-body text-black'>
     <%= microservice.quill.html_safe %>
   </div>
 </div>
<%end%>