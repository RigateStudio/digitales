     <% case microservice.microservice_category.media_type%>
     <% when "image"%>
       <%= image_tag microservice.picture, alt: 'picture', style: "height: 5em", class: "ml-5 border rounded"%>
     <% when "video"%>
      <%=microservice.youtube.html_safe %>
     <% when "sound"%>
      <%= microservice.soundcloude.html_safe  %>
     <% when "text"%>
      <%= microservice.quill.html_safe %>
     <%end%>