<div class="fixed-top d-flex justify-content-end">    
    <%= link_to new_creatrix_registration_path do %> 
        <h5 class="gg-signup gg-title bg-primary mt-4 mr-4 btn btn-primary"><b>Creep in</b></h5>
    <% end %>
</div>
<div style="overflow-x: hidden;">
    <div class="digitales-dot-gg">
       <div class="logo text-center">
         <%=image_tag("text1129.png", alt: "d i g i t a l e s . g g")%>
       </div>
    </div>
    <div style="height: 200px; background-color: black; margin-top: 0px; padding-top: 0px;" class="d-flex flex-column justify-content-start">
